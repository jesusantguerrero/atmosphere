{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./atmosphere"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "doctrine/dbal": "^3.3",
        "ernysans/laraworld": "^1.0",
        "freesgen/atmosphere": "dev-master",
        "guzzlehttp/guzzle": "^7.0.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "insane/journal": "dev-master",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.8",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "php-mime-mail-parser/php-mime-mail-parser": "^7.0",
        "phpoffice/phpspreadsheet": "^1.22",
        "simshaun/recurr": "^4.0",
        "spatie/laravel-onboard": "^2.3",
        "symfony/dom-crawler": "^6.0",
        "tightenco/ziggy": "^1.4.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.3.3",
        "insane/remotr": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
